{
  "name": "UI Color Palette・WCAG-Compliant Color Palette Suite",
  "author": "Aurélien Grimaud",
  "description": "UI Color Palette is a Sketch plugin that creates, manages, deploys, and publishes consistent and accessible color palettes.",
  "homepage": "https://uicp.ylb.lt/website",
  "version": "5.0.0",
  "icon": "./uicp_icon.png",
  "scope": "document",
  "identifier": "com.ui-color-palette.sketch",
  "commands": [
    {
      "name": "UI Color Palette",
      "identifier": "com.ui-color-palette.sketch.run",
      "script": "./runUicp.js",
      "handlers": {
        "run": "onRun",
        "actions": {
          "Shutdown": "onShutdown",
          "SelectionChanged": "onChangeSelection",
          "OpenDocument": "onOpenDocument",
          "CloseDocument": "onCloseDocument"
        }
      }
    }
  ],
  "menu": {
    "title": "UI Color Palette",
    "items": ["com.ui-color-palette.sketch.run"],
    "isRoot": true
  },
  "compatibleVersion": "69.0",
  "bundleVersion": 1
}
